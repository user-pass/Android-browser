<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nimbusbrowser.UrlSearch">

    <EditText
        android:id="@+id/input_search_url"
        android:layout_width="344dp"
        android:layout_height="wrap_content"

        android:layout_below="@+id/app_bar"
        android:layout_alignTop="@+id/app_bar"
        android:layout_alignBottom="@+id/search_url_button"
        android:layout_alignParentStart="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/app_bar"
        android:autofillHints=""
        android:background="?attr/colorButtonNormal"
        android:ems="10"
        android:hint="@string/Search"
        android:inputType="textMultiLine" />

    <Button
        android:id="@+id/search_url_button"
        android:layout_width="67dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/app_bar"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:text="@string/PUSH" />

    <WebView
        android:id="@+id/SearchWebsite"
        android:layout_width="match_parent"
        android:layout_height="685dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="46dp" />
</RelativeLayout>